for(x in 1..3){
    println x
}
def repeat(val, x=10){
    for(i in 0..<x){
        println val
    }
}
//method for loop
repeat(5)
//scope - collection array list
def arg = ["one","two","three","four"]
println arg
println arg.class
println arg[1]
// three methods to append to the array list
arg.add("five")
arg << "six"
arg[6] = "seven"
println arg

//array list with numbers
def no = [1,2,3,4]
def no2 = no +5 //add 5
def no3 = no -[2,3] // remove 2 and 3
println no.class //show the class
println no
println no2
println no3
String st1 = "hello";
String st2 = new String("hello");
if (st1 == st2){
    println("equal");
} else {
    println("not equal");
}
